package com.learn.practice;

public class SingleArrayApproach {
	public static void main(String[] args) {
	int[] arr = new int[7] ;
	 arr[0] = 15;
	 arr[1] = 20;
	 arr[2] = 10;
	 arr[3] = 11;
	 arr[4] = 51;
	 arr[5] = 9;
	 arr[6] = 75;
	 
	 for(int i=0;i<=6;i++)
	 {
		 System.out.println(arr[i]);
	 }

}
}
